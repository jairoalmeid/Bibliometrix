@article{ WOS:000722085100013,
Author = {Guo, Dongen and Xia, Ying and Luo, Xiaobo},
Title = {GAN-Based Semisupervised Scene Classification of Remote Sensing Image},
Journal = {IEEE GEOSCIENCE AND REMOTE SENSING LETTERS},
