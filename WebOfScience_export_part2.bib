@inproceedings{ WOS:000380548200252,
Author = {Shlienkova, Elena},
Book-Author = {Piscitelli, M},
Title = {Museum Quarter as the ``core of urban balance{''} in the post-industrial
   city},
Booktitle = {Heritage and Technology: Mind Knowledge Experience},
Series = {Fabbrica della Conoscenza},
Year = {2015},
Volume = {56},
Pages = {2209-2221},
